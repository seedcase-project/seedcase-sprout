@startuml create-properties-template
!theme seedcase from https://raw.githubusercontent.com/seedcase-project/seedcase-theme/main

state "create_properties_template()" as cpt {
  cpt : - Required arg: path to where to save file

  state arg <<entryPoint>>

  state "DATA_PACKAGE_SPEC_URL" as url 
  url : - Variable of the URL to the\n  Data Package Spec

  state "scrape_url()" as su
  su : - Required arg: url
  su : - Output: JSON
  su : - Scrapes the JSON\n  using web API
  url --> su : string

  state "write_file()" as wf
  wf : - Required arg: string and path
  wf : - Output: path
  arg --> wf : path object
  su --> wf : JSON object

  state return <<exitPoint>>

  wf --> return : path object
}

@enduml
